<h1>Search Results</h1>
<h4>Total Members: <%= @results.count %> </h4>

<% @results[0..24].each do |member| %>
  <div id=<%= member.id %> >
  <p>Name: <%= member.name %></p>
  <img src=<%= member.photo %> style="width:150px;height:150;">
  <p>Allies: 
    <% member.allies.each do |ally| %>
      <%= ally %>, 
    <% end %>
  </p>
  <p>Enemies: 
    <% member.enemies.each do |enemy| %>
      <%= enemy %>, 
    <% end %>
  </p>
  <p>Affiliation: <%= member.affiliation %></p>
  <br>
  </div>
<% end %>